{
  "version": 2,
  "name": "design-system-playground",
  "installCommand": "npm install",
  "buildCommand": "cd apps/playground && npm run build",
  "builds": [
    {
      "src": "apps/playground/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/apps/playground/$1"
    }
  ]
}
